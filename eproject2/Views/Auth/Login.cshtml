@model eproject2.Models.Users
@{

}
<h2>SignIn</h2>
<form asp-action="Login" method="post" enctype="multipart/form-data">
    <div class="form-group">
        
    <label>Email</label>
    <input asp-for="Email" type="email" />
    </div>
    <div class="form-group">
      
    <label>Password</label>
    <input asp-for="Password" type="password"/>
    </div>
    <div class="form-group">

        <button type="submit" class="btn btn-primary" asp-action="Login" asp-controller="Auth">SIGNIN</button>
    </div>
</form>
<a href="@Url.Action("Register", "Auth")">Don't have an account? Register</a>

